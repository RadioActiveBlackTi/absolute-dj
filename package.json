{
  "name": "absolute-dj",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.RadioActiveBlackTi.AbsoluteDJ",
    "productName": "Absolute DJ",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "absol.ico"
    },
    "files": [
      "**/*",
      "!*.ico",
      "!assets/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!dist/*",
      "!configs/*"
    ],
    "extraResources": [
      {
        "from": "assets/",
        "to": "assets/",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "configs/",
        "to": "configs/",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0"
  },
  "dependencies": {
    "json5": "^2.2.3"
  }
}
